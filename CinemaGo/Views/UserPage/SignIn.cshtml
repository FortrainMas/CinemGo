
@{
    ViewData["Title"] = "SignIn";
}

<h1>SignIn</h1>
<center>
    <form asp-controller="UserPage" asp-action="GetNewUser" method="post" enctype="multipart/form-data">
        <input type = "text" name="username" class = "Input" placeholder="Name">
        <input type = "text" name="password" class = "Input" placeholder="Password">
        <input type = "text" name="phoneNumber" class = "Input" placeholder="Phone">
        <input type = "submit" class="sendUserData">
    </form>
</center>
